<Group labelreference="L87" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">fsolve_combsys := proc(Sys, Nprec, x)
  local c, _eq, modSys, realSys, fakeSys, fakeNames, modEqs, newCombEqs, newAddEqs, _t_eqs, _t_req, _t_vars, _negs, i:
  c := 0: fakeSys := {}: realSys := {}; fakeNames := {}: _negs := {}:
  modSys := eval(subs( { Set = proc()
                                 c := c+1:
                                 fakeNames := fakeNames union { cat(PF, c) }:
<Font encoding="UTF-8">                                 _negs := _negs union {\302\240-cat(PF, c)(z) = 0 }:
</Font>                                 fakeSys := fakeSys union { cat(PF, c) = Prod(Atom, Sequence(Atom)) }:
                                 realSys := realSys union { cat(PF, c)(z) = polya(Nprec)(x)(
                                                                               union_truncSeries(Sys)(unlabelled)(Nprec+1)(args[1]))
                                                                              (1)(Nprec) }: # FIXME: check it is zero
                                 Set(Union(getFromUnion(args[1]),cat(PF,c)))                               end
                       }, Sys)):
  modEqs := gfeqns(modSys union fakeSys, labeled, x):
  newCombEqs := {}: newAddEqs := {}:
  for _eq in modEqs
  do
    if (evalb(op(0,lhs(_eq)) in fakeNames)) then newAddEqs := newAddEqs union {_eq}
    else newCombEqs := newCombEqs union {_eq} end:
  end do:
  c := 0:
  for _eq in newCombEqs
  do
    c := c+1:
    newCombEqs:=subs(lhs(_eq)=eval(cat(Eq, c)), newCombEqs):
  od: 
  _t_eqs  := seq(-lhs(_eq) + rhs(_eq), _eq=newCombEqs):
  _t_vars := seq(lhs(_eq), _eq=newCombEqs):
  _t_eqs  := seq(_eq, _eq=subs(_negs, [_t_eqs])):
  _t_eqs  := seq(_eq, _eq=subs({-PF1(z) = 0, -PF2(z) = 0}, [_t_eqs])): # FIXME: check validity?
  _t_eqs  := seq(_eq, _eq=subs(realSys, [_t_eqs])): 
  _t_req  := Determinant(JacobMEqs([_t_eqs], [_t_vars])):
  return fsolve({_t_eqs, _t_req}, {seq(i=0..1, i=_t_vars), z=0.0..0.3}):
end proc:</Text-field>
</Input>
</Group>
